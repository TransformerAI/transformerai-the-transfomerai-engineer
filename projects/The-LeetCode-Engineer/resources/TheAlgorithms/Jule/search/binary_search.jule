fn BinarySearch(s: []int, i: int): (pos: int) {
    let mut b = 0
    let mut e = len(s) - 1
    for b <= e {
        let mid = int(b + (e-b)/2)
        match {
        | s[mid] > i:
            e = mid - 1

        | s[mid] < i:
            b = mid + 1

        |:
            ret mid
        }
    }
    ret -1
}
