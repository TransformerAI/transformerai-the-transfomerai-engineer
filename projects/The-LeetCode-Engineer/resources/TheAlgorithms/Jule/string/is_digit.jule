/*
This function, checks if a string contains only digits.
*/

/*
IsDigit("12345")
true

IsDigit("12345f")
false

IsDigit("123 45")
false
*/

fn IsDigit(digit: str): bool {
    let mut state: bool = false
    for _, n in digit {
        if n >= '0' && n <= '9' {
            state = true
        } else {
            state = false
            break
        }
    }
    ret state
}