/*
This function, checks if a string contains only ASCII characters.
*/

/*
IsAlpha("FooBar")
true

IsAlpha("Foo123")
false

IsAlpha("Foo Bar")
false
*/

fn IsAlpha(s: str): bool {
    let mut state: bool = false
    for _, c in s {
        if c >= 'A' && c <= 'Z' || c >= 'a' && c <= 'z' {
            state = true
        } else {
            state = false
            break
        }
    }
    ret state
}