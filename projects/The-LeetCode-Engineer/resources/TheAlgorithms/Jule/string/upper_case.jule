/*
This function, converts all lowercase characters in a string to uppercase.
*/

/*
upperCase("FooBar")
"FOOBAR"

upperCase("Foo 123")
"FOO 123"
*/

fn UpperCase(mut s: str): str {
    let mut b = []byte(s)
    for i, c in b {
        if c >= 'a' && c <= 'z' {
            b[i] = 'Z' - ('z' - c)
        }
    }
    ret str(b)
}